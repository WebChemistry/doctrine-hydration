{
  "name": "nettrine/hydrator",
  "description": "Convert Doctrine entity to array and conversely",
  "type": "library",
  "require": {
    "php": ">= 7.2",
    "doctrine/orm": "^2.6.0",
    "prowebcraft/dot": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Hydrator\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nettrine\\Test\\": [
        "tests/classes/"
      ],
      "": [
        "tests/entities/"
      ]
    }
  },
  "require-dev": {
    "ninjify/qa": "^0.8.0",
    "ninjify/nunjuck": "^0.2.0",
    "codeception/codeception": "^2.5",
    "nette/di": "^3.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12"
  },
  "minimum-stability": "RC"
}
